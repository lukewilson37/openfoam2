#includeEtc "caseDicts/postProcessing/probes/probes.cfg"

fields (p U);
probeLocations
(
   // each entry is a separate probe 
   // make sure to change (x1 y1 z1) and (x2 y2 z2) to
   // the probe coordinates

   (7 0.5 0.005)
   (7 -.5 0.005)
   (8.5 0.5 0.005)
   (8.5 -.5 0.005)
   (9.5 0.5 0.005)
   (9.5 -.5 0.005)   
   
);

